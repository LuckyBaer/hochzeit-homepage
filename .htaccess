//Rewrite to www
Options +FollowSymLinks
RewriteEngine on
RewriteCond %{HTTP_HOST} ^https://luckybaer.github.io/hochzeit-homepage/[nc]
RewriteRule ^(.*)$ http://www.https://luckybaer.github.io/hochzeit-homepage//$1 [r=301,nc]

//Prevent viewing of .htaccess file
<Files .htaccess>
order allow,deny
deny from all
</Files>

//Prevent directory listings
Options All -Indexes
 
